---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title type="text" xml:lang="en">{{ site.name }}</title>
	<link type="application/atom+xml" href="http://morrisjobke.de/feed.atom" rel="self"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>{{ site.url }}</id>
	<author>
		<name>{{ site.author }}</name>
	</author>
	<rights>Copyright (c) 2013, {{ site.author }}; all rights reserved.</rights>

	{% for post in site.posts limit:20 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://kabum.github.io{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>{{ site.url }}{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
